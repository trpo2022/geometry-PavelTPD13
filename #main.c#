#include <stdio.h>
#include <stdlib.h>

int main(){
  int r = 0, t = 0;
  char i[50];
  char c[100];
  int size = 0;
  printf("Введите название фигуры");
  for(r = 0; r < 99; r++){
    scanf("%c", &c[r]);
    if (c[r] == 59 )  break;
  }
  for(r = 0; r < 49; r++){
    if (c[r] == 40 ){
      i[r] = "/0";
      break;
    }
    size++;
    i[r] = c[r];
  }
  for(r = 0; r < size; r++){
    printf("%c", i[r]);

  }

}
    
  
  
